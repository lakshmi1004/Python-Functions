    "num = input(\"Enter a number: \")\n",
    "def recur_factorial(n):\n",
    "    if n == 1:\n",
    "        return n\n",
    "    elif n < 1:\n",
    "        return (\"NA\")\n",
    "    else:\n",
    "        return n*recur_factorial(n-1)\n",
    "        \n",
    "print (recur_factorial(int(num)))  "
   ]
